{
    "name": "godbout/alfred-kat",
    "description": "Search and open magnet links on KAT directly from Alfred.",
    "keywords": [
        "alfred",
        "workflow",
        "alfred-workflow",
        "torrents",
        "kickass",
        "kat"
    ],
    "homepage": "https://github.com/godbout/alfred-kat",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Guill Lo",
            "email": "guill@sleeplessmind.com.mo",
            "role": "Craftsman"
        }
    ],
    "require": {
        "php": "^7.2",
        "godbout/alfred-workflow-workflow": "^1.0",
        "fabpot/goutte": "^3.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/css-selector": "^4.2",
        "guzzlehttp/guzzle": "^6.3"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "codedungeon/phpunit-result-printer": "^0.27.0"
    },
    "autoload": {
        "psr-4": {
            "Godbout\\Alfred\\Kat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --color=always",
        "format": "php-cs-fixer fix src tests -vvv --config=.php_cs.dist --ansi"
    }
}
